. testing.sh

# testing "test name" "command" "expected result" "file input" "stdin"

testing "xargs stops on underscore" \
	"xargs" \
	"a\n" \
	"" "a\n_\nb\n"

testing "xargs -e without param" \
	"xargs -e" \
	"a _ b\n" \
	"" "a\n_\nb\n"

exit $FAILCOUNT
